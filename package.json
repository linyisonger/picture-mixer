{
  "name": "picture-mixer",
  "version": "1.0.3a2",
  "description": "这是一个很'常用'的图片混合组件",
  "main": "dist/index.js",
  "keywords": [
    "typescript",
    "微信小程序"
  ],
  "scripts": {
    "dev": "gulp dev --develop",
    "watch": "gulp watch --develop --watch",
    "build": "gulp",
    "dist": "npm run build",
    "clean-dev": "gulp clean --develop",
    "clean": "gulp clean",
    "test": "jest --bail",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --bail",
    "coverage": "jest ./test/* --coverage --bail"
  },
  "miniprogram": "dist",
  "files": [
    "dist"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "testURL": "https://jest.test",
    "collectCoverageFrom": [
      "dist/**/*.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linyisonger/picture-mixer.git"
  },
  "author": "linyisonger",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-env": "^1.7.0",
    "colors": "^1.3.1",
    "gulp": "^4.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-if": "^2.0.2",
    "gulp-install": "^1.1.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "jest": "^23.5.0",
    "miniprogram-api-typings": "^2.12.0",
    "miniprogram-simulate": "^1.2.5",
    "thread-loader": "^2.1.3",
    "through2": "^2.0.3",
    "ts-loader": "^7.0.0",
    "typescript": "^3.8.3",
    "vinyl": "^2.2.0",
    "webpack": "^4.29.5",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "ru-toolkit-mathematics": "^1.0.9-a1"
  }
}
